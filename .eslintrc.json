{
  "env": {
    "node": true,
    "es6": true,
    "jasmine": true,
    "mocha": true
  },
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module"
  },
  "root": true,
  "plugins": [
    "unicorn",
    "lodash"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:unicorn/recommended",
    "plugin:lodash/recommended",
    "xo-space",
    "airbnb"
  ],
  "rules": {
    "unicorn/no-abusive-eslint-disable": 0,
    "lodash/prefer-lodash-method":0,
    "lodash/import-scope": 0
  }
}
